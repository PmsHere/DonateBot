{
  "name": "Donate Bot",
  "description": "Simple Telegram Bot to tell How to Donate you",
  "keywords": [
    "telegram",
    "donate",
    "plugin",
    "telegram-bot",
    "productivity",
    "jayantkageri"
  ],
  "repository": "https://github.com/jayantkageri/DonateBot",
  "website": "jayantkageri.ml",
  "success_url": "jayantkageri.ml",
  "env": {
    "ENV": {
      "description": "Setting this to ANYTHING will enable heroku.",
      "value": "ANYTHING"
    },
    "TOKEN": {
      "description": "Your Bot Token.",
      "value": ""
    },
    "API_ID": {
      "description": "Get this value from my.telegram.org! Please do not steal",
      "value": ""
    },
    "DONATE_TEXT": {
      "description": "Your Donation Text, Format Markdown",
      "value": "",
      "required": false
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org! Please do not steal",
      "value": ""
    }
  },
  "buildpacks": [
    {
      "url": "heroku/python"
    }
 ],
"addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "12"
      }
    }
  ]
}
